bram _mmap__{{ module.name }}_
    init 0
    abits {{ module.ports.waddr|length }}
    dbits 1
    groups 2
    ports 1 1
    wrmode 1 0
    enable 1 1
    transp 0 0
    clocks 1 1
    clkpol 1 1
endbram

match _mmap__{{ module.name }}_
    make_outreg
    {%- if not_last %}
    or_next_if_better
    {%- endif %}
endmatch


